Cell[BoxData[
 RowBox[{
  RowBox[{"padSelectedExons", "[", 
   RowBox[{"inputFile_", ",", "outputFile_", ",", "padTo_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "data", ",", " ", "paddedData", ",", "length", ",", "nucleotidesNeeded", 
      " ", ",", "upstream", ",", "downstream"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"data", " ", "=", " ", 
      RowBox[{"SemanticImport", "@", "inputFile"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"DeleteFile", "[", "inputFile", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"paddedData", " ", "=", " ", 
      RowBox[{"data", "[", "\[IndentingNewLine]", 
       RowBox[{"All", ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"#", "[", 
            RowBox[{"[", "1", "]"}], "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"length", "=", 
             RowBox[{
              RowBox[{"#", "[", 
               RowBox[{"[", "3", "]"}], "]"}], "-", 
              RowBox[{"#", "[", 
               RowBox[{"[", "2", "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"nucleotidesNeeded", "=", 
             RowBox[{"padTo", "-", "length"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"upstream", "=", 
             RowBox[{"RandomInteger", "[", 
              RowBox[{"{", 
               RowBox[{"1", ",", "nucleotidesNeeded"}], "}"}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"#", "[", 
                 RowBox[{"[", "2", "]"}], "]"}], "-", "upstream"}], " ", "<", 
               "0"}], ",", " ", 
              RowBox[{"upstream", " ", "+", " ", 
               RowBox[{"Abs", "[", 
                RowBox[{
                 RowBox[{"#", "[", 
                  RowBox[{"[", "2", "]"}], "]"}], "-", "upstream"}], 
                "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
            RowBox[{"downstream", " ", "=", " ", 
             RowBox[{"(", 
              RowBox[{"nucleotidesNeeded", " ", "-", " ", "upstream"}], 
              ")"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"#", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "-", "upstream"}]}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"#", "[", 
             RowBox[{"[", "3", "]"}], "]"}], "+", "downstream"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"#", "[", 
            RowBox[{"[", "4", "]"}], "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{"#", "[", 
            RowBox[{"[", "5", "]"}], "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{"#", "[", 
            RowBox[{"[", "6", "]"}], "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{"#", "[", 
            RowBox[{"[", "2", "]"}], "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{"#", "[", 
            RowBox[{"[", "3", "]"}], "]"}]}], "\[IndentingNewLine]", "}"}], 
         "&"}]}], "\[IndentingNewLine]", "]"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"Export", "[", 
      RowBox[{"outputFile", ",", 
       RowBox[{"Normal", "@", "paddedData"}], ",", "\"\<TSV\>\""}], "]"}], 
     ";"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
