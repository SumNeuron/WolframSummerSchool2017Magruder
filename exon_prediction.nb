Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Options", "[", "AnnotatedFastaFileToDataset", "]"}], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\"\<annotationModification\>\"", "\[Rule]", "None"}], ",", " ", 
      RowBox[{"\"\<sequenceModification\>\"", "\[Rule]", "None"}]}], "}"}]}], 
   ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"AnnotatedFastaFileToDataset", "[", 
   RowBox[{"inputFile_", ",", "outputFile_", ",", 
    RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"lineIndex", "=", "0"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ProgressIndicator", "[", 
      RowBox[{
       RowBox[{"Dynamic", "[", "lineIndex", "]"}], ",", "Indeterminate"}], 
      "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Ensure", " ", "Streams", " ", "are", " ", "not", " ", "open"}],
       "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Quiet", "[", 
      RowBox[{"Close", "[", "inputFile", "]"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Quiet", "[", 
      RowBox[{"Close", "[", "outputFile", "]"}], "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Module", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"inputStream", "=", 
          RowBox[{"OpenRead", "[", "inputFile", "]"}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"outputStream", "=", 
          RowBox[{"OpenWrite", "[", "outputFile", "]"}]}], ",", 
         "\[IndentingNewLine]", "annotation", ",", "\[IndentingNewLine]", 
         "sequence", ",", "\[IndentingNewLine]", "annotationAssociation", ",",
          "\[IndentingNewLine]", "sequenceAssociation"}], 
        "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"WriteString", "[", 
         RowBox[{"outputStream", ",", "\"\<{\>\""}], "]"}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"While", "[", 
         RowBox[{"True", ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"annotation", "=", 
            RowBox[{"ReadLine", "[", "inputStream", "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"sequence", "=", 
            RowBox[{"ReadLine", "[", "inputStream", "]"}]}], ";", 
           "\[IndentingNewLine]", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"annotation", " ", "\[Equal]", " ", "EndOfFile"}], ",", 
             RowBox[{"Break", "[", "]"}]}], "]"}], ";", "\[IndentingNewLine]",
            "\[IndentingNewLine]", 
           RowBox[{"(*", " ", 
            RowBox[{
            "Add", " ", "comma", " ", "if", " ", "not", " ", "first", " ", 
             "element"}], " ", "*)"}], "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"lineIndex", "\[NotEqual]", " ", "0"}], ",", 
             RowBox[{"WriteString", "[", 
              RowBox[{"outputStream", ",", "\"\<,\>\""}], "]"}]}], "]"}], ";",
            "\[IndentingNewLine]", 
           RowBox[{"lineIndex", "+=", "2"}], ";", "\[IndentingNewLine]", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
              RowBox[{
              "OptionValue", "[", "\"\<annotationModification\>\"", "]"}], "=!=",
               "None"}], ",", "\[IndentingNewLine]", 
             RowBox[{"annotationAssociation", "=", 
              RowBox[{
               RowBox[{
               "OptionValue", "[", "\"\<annotationModification\>\"", "]"}], 
               "[", "annotation", "]"}]}], ",", "\[IndentingNewLine]", 
             RowBox[{"annotationAssociation", " ", "=", " ", 
              RowBox[{"<|", 
               RowBox[{"\"\<Annotation\>\"", "\[Rule]", " ", "annotation"}], 
               "|>"}]}]}], "\[IndentingNewLine]", "]"}], ";", 
           "\[IndentingNewLine]", "\[IndentingNewLine]", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
              RowBox[{
              "OptionValue", "[", "\"\<sequenceModification\>\"", "]"}], "=!=",
               "None"}], ",", "\[IndentingNewLine]", 
             RowBox[{"sequenceAssociation", "=", 
