bcdb0d5a2abb"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"selectRandomExons", "::", "usage"}], "=", 
   "\"\<selectRandomExons[inputFile, outputFile, numberOfExons, \
minExonLength, maxExonLength] takes in a .bed file as 'inputFile' where the \
second and thrid columns of the file are the start/stop of the sequences. The \
results will be stored in the `outputFile`.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"selectRandomExons", "[", 
    RowBox[{
    "inputFile_", ",", "outputFile_", ",", "numberOfExons_", ",", 
     "minExonLength_", ",", "maxExonLength_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "setNumLinesToGet", ",", " ", "awkBegin", ",", "exonsLessThanLength", 
       ",", "exonsGreatThanLength", ",", "getRandomLines", ",", "awkEnd", ",",
        "command"}], "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"setNumLinesToGet", " ", "=", " ", 
       RowBox[{"\"\<-v count=\>\"", " ", "<>", " ", 
        RowBox[{"ToString", "[", "numberOfExons", "]"}], " ", "<>", " ", 
        "\"\< \>\""}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"awkBegin", " ", "=", " ", "\"\<'BEGIN { srand() } \>\""}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"exonsLessThanLength", " ", "=", " ", 
       RowBox[{"\"\< $3 - $2 < \>\"", " ", "<>", " ", 
        RowBox[{"ToString", "[", "maxExonLength", "]"}], " ", "<>", " ", 
        "\"\< \>\""}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"exonsGreatThanLength", " ", "=", " ", 
       RowBox[{"\"\< $3 - $2 > \>\"", " ", "<>", " ", 
        RowBox[{"ToString", "[", "minExonLength", "]"}], "<>", " ", 
        "\"\< \>\""}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
      "getRandomLines", " ", "=", " ", 
       "\"\<rand() < count / ++n {if (n <= count) {s[n] = $0 } else { \
s[1+int(rand()*count)] = $0 } } \>\""}], ";", "\[IndentingNewLine]", 
      RowBox[{
      "awkEnd", " ", "=", " ", "\"\<END { for (i in s) print s[i] }' \>\""}], 
      ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"command", "=", 
       RowBox[{
       "\"\<awk \>\"", " ", "<>", " ", "setNumLinesToGet", " ", "<>", " ", 
        "awkBegin", " ", "<>", " ", "exonsGreatThanLength", " ", "<>", 
        "\"\<&& \>\"", " ", "<>", " ", "exonsLessThanLength", " ", "<>", " ", 
        "\"\<&& \>\"", " ", "<>", "getRandomLines", " ", "<>", " ", "awkEnd", 
        " ", "<>", " ", "inputFile", " ", "<>", " ", "\"\< > \>\"", " ", "<>",
         " ", "outputFile"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Run", "[", "command", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]}], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
 CellChangeTimes->{{3.707227635858971*^9, 3.707227761540481*^9}, {
   3.7072278327764597`*^9, 3.7072278435425663`*^9}, {3.7072280327893753`*^9, 
