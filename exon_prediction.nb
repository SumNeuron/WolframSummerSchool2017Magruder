        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"WriteString", "[", 
         RowBox[{"outputStream", ",", "\"\<}\>\""}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Close", "[", "inputStream", "]"}], ";", 
